{
  "name": "@unionpdf/engines",
  "version": "0.17.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "core components of unionpdf",
  "source": "./src/index.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "import": "./dist/esm/index.mjs",
    "require": "./dist/cjs/index.js"
  },
  "scripts": {
    "doc": "typedoc ./src/index.ts",
    "typecheck": "tsc --noEmit",
    "start": "parcel --no-cache ./demo/index.html",
    "prebuild": "npm run clean",
    "build": "parcel build",
    "postbuild": "cpy ./src/pdfium/*.d.ts ./dist/types --no-overwriten --flat",
    "clean": "del-cli --force dist .parcel-cache",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jichang/unionpdf.git"
  },
  "keywords": [
    "PDF",
    "Engines",
    "Pdfium"
  ],
  "files": [
    "dist/cjs",
    "dist/esm",
    "dist/types"
  ],
  "author": "Ji Chang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jichang/unionpdf/issues"
  },
  "homepage": "https://github.com/jichang/unionpdf#readme",
  "devDependencies": {
    "@parcel/packager-ts": "^2.9.1",
    "@parcel/transformer-typescript-types": "^2.9.1",
    "@types/jest": "^29.5.1",
    "cpy-cli": "^4.2.0",
    "crypto-browserify": "^3.12.0",
    "del-cli": "^5.0.0",
    "events": "^3.3.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "parcel": "^2.9.1",
    "path-browserify": "^1.0.1",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "~5.0.4"
  },
  "dependencies": {
    "@unionpdf/models": "^0.17.0",
    "buffer": "^6.0.3",
    "process": "^0.11.10"
  }
}
