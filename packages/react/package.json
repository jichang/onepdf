{
  "name": "@unionpdf/react",
  "version": "0.16.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "core components of unionpdf",
  "source": "./src/index.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "import": "./dist/esm/index.mjs",
    "require": "./dist/cjs/index.js"
  },
  "scripts": {
    "api-extractor": "api-extractor",
    "doc": "api-extractor run --local --verbose && api-documenter markdown --input-folder ./temp --output-folder ../../docs/react",
    "typecheck": "tsc --noEmit",
    "start": "parcel --no-cache ./demo/index.html",
    "build": "parcel build",
    "clean": "del-cli --force dist .parcel-cache",
    "test": "jest"
  },
  "files": [
    "dist/cjs",
    "dist/esm",
    "dist/types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jichang/unionpdf.git"
  },
  "keywords": [
    "PDF",
    "Viewer",
    "React",
    "Editor",
    "Pdfium"
  ],
  "author": "Ji Chang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jichang/unionpdf/issues"
  },
  "homepage": "https://github.com/jichang/unionpdf#readme",
  "devDependencies": {
    "@microsoft/api-documenter": "^7.22.6",
    "@microsoft/api-extractor": "^7.35.0",
    "@parcel/packager-ts": "^2.9.1",
    "@parcel/transformer-typescript-types": "^2.9.1",
    "@shopify/jest-dom-mocks": "^5.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "del-cli": "^5.0.0",
    "jest": "^29.5.0",
    "jest-canvas-mock": "^2.5.1",
    "jest-css-modules-transform": "^4.4.2",
    "jest-environment-jsdom": "^29.5.0",
    "parcel": "^2.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.0",
    "typescript": "~5.0.4"
  },
  "dependencies": {
    "@types/classnames": "^2.3.1",
    "@unionpdf/models": "^0.16.0",
    "classnames": "^2.3.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
