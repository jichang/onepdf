{
  "name": "@unionpdf/pdfium",
  "version": "0.24.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "Pdfium webassebmly for web platform",
  "source": "./src/index.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./pdfium.wasm": {
      "import": "./dist/esm/pdfium.f83f3440.wasm",
      "require": "./dist/cjs/pdfium.f83f3440.wasm"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "parcel build",
    "clean": "del-cli --force dist .parcel-cache",
    "postbuild": "npm run copy-types",
    "copy-types": "cpy src/pdfium.d.ts dist/types/ --flat && cpy ./dist/types/* ./dist/cjs --flat && cpy ./dist/types/* ./dist/esm --flat",
    "build:wasm": "make build && cpy ./docker/src/pdifum.js ./src --flat && cpy ./docker/src/pdfium.wasm ./src --flat",
    "build:all": "npm run build:wasm && npm run build",
    "test": "jest --passWithNoTests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jichang/unionpdf.git"
  },
  "keywords": [
    "pdfium",
    "webassembly"
  ],
  "files": [
    "src/cjs",
    "src/esm",
    "src/types"
  ],
  "author": "Ji Chang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jichang/unionpdf/issues"
  },
  "homepage": "https://github.com/jichang/unionpdf#readme",
  "devDependencies": {
    "buffer": "^6.0.3",
    "comlink": "^4.4.1",
    "cpy-cli": "^5.0.0",
    "crypto-browserify": "^3.12.0",
    "del-cli": "^5.1.0",
    "jest": "^29.7.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@types/emscripten": "^1.39.12",
    "parcel": "^2.12.0"
  }
}
